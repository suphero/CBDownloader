<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <configSections>
        <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" >
            <section name="CBDownloader.DownloaderSettings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
        </sectionGroup>
    </configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
    <applicationSettings>
        <CBDownloader.DownloaderSettings>
            <setting name="DownloadPath" serializeAs="String">
                <value>##DownloadPath##</value>
            </setting>
            <setting name="PlaylistFilePath" serializeAs="String">
                <value>##PlaylistFilePath##</value>
            </setting>
            <setting name="PlaylistFileNameToSave" serializeAs="String">
                <value>playlist.txt</value>
            </setting>
            <setting name="UseRegEx" serializeAs="String">
                <value>True</value>
            </setting>
            <setting name="RegExPattern" serializeAs="String">
                <value>.*?\\d+.*?(\\d+)</value>
            </setting>
        </CBDownloader.DownloaderSettings>
    </applicationSettings>
</configuration>